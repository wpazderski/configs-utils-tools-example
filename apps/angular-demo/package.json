{
    "name": "@wpazderski/angular-demo",
    "private": true,
    "version": "1.0.0",
    "author": "Witold Pazderski",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "lint-check": "eslint ./ --flag unstable_native_nodejs_ts_config --flag unstable_config_lookup_from_file --max-warnings 0",
        "lint-fix": "eslint ./ --flag unstable_native_nodejs_ts_config --flag unstable_config_lookup_from_file --max-warnings 0 --fix",
        "tsc-check": "tsc -b --noEmit",
        "check-all": "run-s lint-check tsc-check",
        "pre-commit-checks": "run-s check-all",
        "ng": "ng",
        "dev": "ng serve",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test"
    },
    "dependencies": {
        "@angular/common": "^20.1.6",
        "@angular/compiler": "^20.1.6",
        "@angular/core": "^20.1.6",
        "@angular/forms": "^20.1.6",
        "@angular/platform-browser": "^20.1.6",
        "@angular/router": "^20.1.6",
        "rxjs": "~7.8.2",
        "tslib": "^2.8.1",
        "zone.js": "~0.15.1"
    },
    "devDependencies": {
        "@angular/build": "^20.1.6",
        "@angular/cli": "^20.1.6",
        "@angular/compiler-cli": "^20.1.6",
        "@types/jasmine": "~5.1.8",
        "@wpazderski/eslint-config": "^1.0.5",
        "@wpazderski/typescript-config": "^1.0.4",
        "eslint": "^9.33.0",
        "jasmine-core": "~5.9.0",
        "karma": "~6.4.4",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.1",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "less": "^4.4.0",
        "typescript": "~5.9.2"
    },
    "packageManager": "pnpm@10.11.0",
    "engines": {
        "node": ">=24.0.0",
        "pnpm": ">=10.0.0",
        "npm": "please-use-pnpm",
        "yarn": "please-use-pnpm"
    }
}
